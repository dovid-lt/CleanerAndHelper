{
  "name": "Cleaner And Helper",
  "version": "1.2.1",
  "description": "Prevents annoying advertisements and improves convenience, designed for Netfree users.",
  "manifest_version": 3,
  "icons": {
    "16": "media2/16.png",
    "48": "media2/32.png",
    "128": "media2/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.jdn.co.il/*"
      ],
      "run_at": "document_start",
      "js": [
        "general-mutation.js",
        "jdn.js"
      ]
    },
    {
      "matches": [
        "https://hm-news.co.il/*"
      ],
      "run_at": "document_start",
      "js": [
        "general-mutation.js",
        "hm-news.js"
      ]
    },
    {
      "matches": [
        "https://www.kore.co.il/*"
      ],
      "run_at": "document_start",
      "js": [
        "general-mutation.js",
        "kore.js"
      ]
    },
    {
      "matches": [
        "https://www.kore.co.il/flashNews*"
      ],
      "run_at": "document_start",
      "js": [
        "kore-flashNews.js"
      ],
      "css": [
        "kore-flashNews.css"
      ]
    },
    {
      "matches": [
        "https://www.bahazit.co.il/*"
      ],
      "run_at": "document_start",
      "js": [
        "general-mutation.js",
        "bahazit.js"
      ]
    },
    {
      "matches": [
        "https://www.prog.co.il/*"
      ],
      "run_at": "document_start",
      "js": [
        "general-mutation.js",
        "prog.js"
      ],
      "css": [
        "prog.css"
      ]
    },
    {
      "matches": [
        "https://kcm.fm/*"
      ],
      "run_at": "document_idle",
      "js": [
        "kcm.js"
      ]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["kcm-em.js"],
    "matches": ["https://kcm.fm/*"]
  }],
  "permissions": [
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "https://www.jdn.co.il/*",
    "https://info.jdn.co.il/*",
    "https://hm-news.co.il/*",
    "https://www.kore.co.il/*",
    "https://www.prog.co.il/*",
    "https://kcm.fm/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}