{
  "name": "Cleaner And Helper",
  "version": "0.0.1",
  "description": "Prevents annoying advertisements and improves convenience, designed for Netfree users",
  "manifest_version": 2,
  "icons": {
    "16": "media2/16.png",
    "48": "media2/32.png",
    "128": "media2/128.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://www.jdn.co.il/*"  ],
      "run_at": "document_end",
      "js": [ "blocker.js" ],
      "css": [ "style.css" ]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.jdn.co.il/*",
    "https://info.jdn.co.il/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
